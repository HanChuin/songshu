}

.nologin {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  padding-top: 30rpx;
  align-items: center;
  color: #e6792b;
  margin-bottom: 20rpx;
}

.loginbtn {
  width: 320rpx;
  height: 80rpx;
  margin-top: 30rpx;
  border: #e6792b 1px solid;
  border-radius: 40rpx;
  display: flex;
