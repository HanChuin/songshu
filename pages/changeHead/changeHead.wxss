/* pages/changeHead/changeHead.wxss */
page {
  background: #fff;
}
.box {
  width: 100%;
  padding-left: 40rpx;
}
.box>view {
  width: 100%;
  height: 90rpx;
  border-bottom: 1px solid #ccc;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right: 20rpx;
}
.box .head {
  height: 140rpx;
}
.headImg {
  width: 120rpx;
  height: 120rpx;
}

.box2 {
  width: 100%;
  height: 70rpx;
  border-bottom: 1px solid #FF902E;
  padding: 0 10rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #333;
  margin-top: 30rpx;
}

.box2 input {
  width: 80%;
  height: 70rpx;
}
.box2 text {
  margin-right: 20rpx;
}


.btnview {
  margin-left: 12%;
  margin-right: 12%;
  width: 76%;
  margin-top: 50rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background: #ff902e;
  color: #fff;
  font-size: 28rpx;
  line-height: 80rpx;
  text-align: center;
}