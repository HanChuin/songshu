.tabbar_view {
  display: flex;
  flex-direction: column;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 777;
  width: 100%;
  height: auto;
}

.tabbar_box {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 120rpx;
}

.head-view {
  height: 200rpx;
  padding-top: 40rpx;
}

.header {
  width: 100%;
  display: flex;
  justify-content: center;
  margin-bottom: 20rpx;
}

.head {
  width: 120rpx;
  border-radius: 60rpx;
}

.nickName {
  text-align: center;
}

.tabg {
  width: 100%;
  height: 140rpx;
  position: absolute;
  align-self: flex-start;
  left: 0rpx;
  top: 0;
  z-index: 700;
}

.middle-wrapper {
  position: absolute;
  right: 310rpx;
  bottom: 0;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
}

.middle-wrapper.iphoneX-height {
  bottom: 66rpx;
}

.tabbar_nav {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  height: 100%;
  z-index: 999;
}

.mid {
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
  margin-top: -52rpx;
  background: #ed8122;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
  box-shadow: 0rpx 10rpx 5rpx rgba(80, 80, 80, 0.2);
}

.special-text {
  font-size: 24rpx;
  color: #ee9643;
}

.normal-text {
  font-size: 24rpx;
  color: #000;
}

.tabbar_icon {
  width: 50rpx;
  height: 50rpx;
}

.tabbar_icon_mid {
  width: 50%;
  height: 50%;
}

.special-wrapper {
  position: absolute;
  left: 77rpx;
  top: -36rpx;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
  text-align: center;
  box-sizing: border-box;
  padding: 6rpx;
}

.special-wrapper .tabbar_icon {
  width: 84rpx;
  height: 84rpx;
}

.special-text-wrapper {
  width: 40rpx;
  height: 40rpx;
}

.cover {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 100;
}

.qrCode {
  width: 100vw;
  height: 70vh;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  z-index: 111;
}

.qrCode-view {
  position: relative;
  z-index: 999;
}

.tabbar_bottom {
  width: 100%;
  height: 0rpx;
}

.tabbar_bottomx {
  width: 100%;
  height: 30rpx;
  background-color: white;
}
